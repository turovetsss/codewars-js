{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "linebreak-style": "off",
        "no-console": "off",
        "indent": ["error", 4],
        "no-plusplus":"off"
    }
}
